async (req, res) => {
        try {
            const cliente = await Clientes.findOne({
                where: {
                    whatsapp: req.body.whatsapp // Usa o e-mail que veio do frontend
                }
            });whatsapp
          if(req.body.whatsapp == cliente.whatsapp && req.body.senha == cliente.senha){
                res.json(cliente)
            } else {
                res.status(400).send('whatsapp ou senha incorreta')
            }
        } catch (error) {
            res.status(500).send(error.messwhatsappage);
        }
    }