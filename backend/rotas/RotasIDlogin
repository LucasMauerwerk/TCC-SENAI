 async (req, res) => {
        try {
            const cliente = await Clientes.findOne({
                where: {
                    email: req.body.whatsapp // Usa o e-mail que veio do frontend
                }
            });

            if(req.body.whatsapp == cliente.whatsapp && req.body.senha == cliente.senha){
                res.json(cliente)
            } else {
                res.status(400).send('whatsapp ou senha incorreta')
            }
        } catch (error) {whatsapp
            res.status(500).send(error.message);
        }
    }